              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Description"
          },
          "annotation_refs": {
            "items": {
              "$ref": "#/$defs/AnnotationRef"
            },
            "title": "Annotation Refs",
            "type": "array"
          },
          "id": {
            "default": "__auto_sequence__",
            "pattern": "(urn:lsid:([\\w\\-\\.]+\\.[\\w\\-\\.]+)+:Annotation:\\S+)|(Annotation:\\S+)",
            "title": "Id",
            "type": "string"
          },
          "namespace": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Namespace"
          },
          "annotator": {
            "anyOf": [
              {
                "pattern": "(urn:lsid:([\\w\\-\\.]+\\.[\\w\\-\\.]+)+:Experimenter:\\S+)|(Experimenter:\\S+)",
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Annotator"
          },
          "value": {
            "$ref": "#/$defs/Value"
          }
        },
        "required": [
          "value"
        ],
        "title": "XMLAnnotation",
        "type": "object"
      }
    "properties": {
      "rights": {
        "anyOf": [
          {
            "$ref": "#/$defs/Rights"
          },
          {
            "type": "null"
          }
        ],
        "default": null
      },
      "projects": {
